	</tag>
	<tag>
		<tag-name>area</tag-name>
		<component>
			<component-type>AreaComponent</component-type>
			<handler-class />
		</component>
		<attribute>
			<name>id</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>value</name>
			<required>true</required>
			<type>com.highcharts.chart.area.AreaChart</type>
		</attribute>
		<attribute>
			<name>width</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>height</name>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	<tag>
		<tag-name>column</tag-name>
		<component>
			<component-type>ColumnComponent</component-type>
			<handler-class />
		</component>
		<attribute>
			<name>id</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>value</name>
			<required>true</required>
			<type>com.highcharts.chart.column.ColumnChart</type>
		</attribute>
		<attribute>
			<name>width</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>height</name>
			<type>java.lang.String</type>
		</attribute>
