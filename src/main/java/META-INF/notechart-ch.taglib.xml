		<component>
			<component-type>PieComponent</component-type>
			<handler-class />
		</component>
		<attribute>
			<name>id</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>value</name>
			<required>true</required>
			<type>com.highcharts.chart.pie.PieChart</type>
		</attribute>
		<attribute>
			<name>width</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>height</name>
			<type>java.lang.String</type>
